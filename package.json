    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-animatable": "^1.3.3",
    "react-native-applovin-max": "^3.3.0",
    "react-native-banner-carousel": "^1.0.3",
    "react-native-color-picker": "^0.6.0",
    "react-native-email-link": "^1.14.1",
    "react-native-flip-page": "^0.5.0",
    "react-native-google-mobile-ads": "^12.1.2",
    "react-native-in-app-review": "^4.3.3",
